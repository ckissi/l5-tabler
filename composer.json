{
    "name": "ckissi/l5-tabler",
    "description": "Tabler template for Laravel 5.*",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "baorv",
            "email": "roanvanbao@gmail.com"
        },
        {
            "name": "Csaba Kissi",
            "email": "ckissi@gmail.com"
        }
    ],
    "require": {
        "php": ">=5.4.0",
        "tabler/tabler": "^0.0.32",
        "illuminate/support": ">=5.0",
        "illuminate/auth": ">=5.0",
        "lavary/laravel-menu": ">=1.6.1",
        "laravelcollective/html": ">=5.0"
    },
    "require-dev": {
        "phpunit/phpunit": ">=4.0",
        "orchestra/testbench": ">=3.0"
    },
    "autoload": {
        "psr-4": {
            "Pingecom\\Tabler\\": "src/"
        },
        "files": [
            "src/helpers.php"
        ]
    },
    "autoload-dev": {
        "classmap": [
            "tests/TestCase.php"
        ],
        "psr-4": {
            "Pingecom\\Tabler\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Pingecom\\Tabler\\Providers\\TablerServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
